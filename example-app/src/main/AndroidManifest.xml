<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.dean.jraw.android.example">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="net.dean.jraw.android.REDDIT_USERNAME"
            android:value="thatJavaNerd" />
        <meta-data
            android:name="net.dean.jraw.android.CLIENT_ID"
            android:value="ZKe8PaIqR8Jzcg" />
        <meta-data
            android:name="net.dean.jraw.android.REDIRECT_URL"
            android:value="https://github.com/mattbdean/JRAW-Android" />

        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewUserActivity" />
        <activity android:name=".UserOverviewActivity" />
    </application>
</manifest>